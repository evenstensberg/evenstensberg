{
  "name": "evenstensberg",
  "version": "1.0.0",
  "description": "My personal site",
  "main": "server/bin/server.js",
  "scripts": {
    "prestart": "npm run build",
    "dev": "cross-env NODE_ENV=development webpack --config  .webpack/webpack.client.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config .webpack/webpack.client.prod.js --progress",
    "hot": "cross-env NODE_ENV=hot webpack-dev-server --config .webpack/webpack.hot.js",
    "start": "go run ./app.go",
    "serve": "npm run build && npm run start",
    "deploy": "gcloud app deploy",
    "system-test": "repo-tools test app",
    "test": "npm run system-test",
    "e2e-test": "repo-tools test deploy",
    "css": "cross-env NODE_ENV=development webpack --config  .webpack/webpack.test.js"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evenstensberg/evenstensberg"
  },
  "author": "Even Stensberg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evenstensberg/evenstensberg"
  },
  "homepage": "https://github.com/evenstensberg/evenstensberg#readme",
  "dependencies": {
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@google-cloud/storage": "^5.1.0",
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "autoprefixer": "^9.8.0",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "clsx": "^1.1.1",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "image-webpack-loader": "^6.0.0",
    "markdown-loader": "^5.1.0",
    "markdown-to-jsx": "^6.11.4",
    "mini-css-extract-plugin": "^0.9.0",
    "multer": "^1.4.2",
    "node-sass": "^4.14.1",
    "npm": "^6.14.5",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "precss": "^4.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "sass-loader": "^8.0.2",
    "serve-favicon": "^2.5.0",
    "style-loader": "^1.2.1",
    "svg-inline-loader": "^0.8.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.10.1",
