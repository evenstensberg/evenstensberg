{
  "name": "evenstensberg",
  "version": "1.0.0",
  "description": "My personal site",
  "main": "server/bin/server.js",
  "scripts": {
    "dev": "webpack-dev-server --config .webpack/webpack.client.dev.js",
    "build": "npm run build:client && npm run build:server",
    "build:server": "cross-env NODE_ENV=production webpack --config .webpack/webpack.server.prod.js --progress",
    "build:client": "cross-env NODE_ENV=production webpack --config .webpack/webpack.client.prod.js --progress",
    "start": "cd lib/server && node bin/server.js",
    "serve": "npm run build && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evenstensberg/evenstensberg"
  },
  "author": "Even Stensberg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evenstensberg/evenstensberg"
  },
  "homepage": "https://github.com/evenstensberg/evenstensberg#readme",
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "cross-env": "^5.1.4",
    "ejs": "^2.5.9",
    "express": "^4.16.3",
    "npm": "^6.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.1.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0"
  },
